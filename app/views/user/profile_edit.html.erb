

<%= form_with model:@user, url: user_profile_update_path, local:true  do |f| %>
  
  <div class="field">
    <%= f.file_field :avatar %>
  </div>
  
  <div class="field">
    <%= f.label :name %><br/>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :profile %><br/>
    <%= f.text_field :profile %>
  </div>

  <div class="actions">
    <%= f.submit "更新" %>
  </div>
  
<% end %>





<%= link_to "Back", :back %>
