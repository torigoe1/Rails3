新規登録
<%= form_with model:@room, url: room_create_path, local:true  do |f| %>

    
    <div class="field">
      <%= f.label :name %><br/>
      <%= f.text_field :name %>
    </div>
  
    <div class="field">
      <%= f.label :introduce %><br/>
      <%= f.text_area :introduce %>
    </div>

    <div class="field">
    <%= f.label :price %><br/>
    <%= f.text_field :price %>
    </div>

    <div class="field">
    <%= f.label :address %><br/>
    <%= f.text_field :address %>
    </div>

    <div class="field">
    <%= f.label :image_name %><br/>
    <%= f.file_field :image_name %>
    </div>
  
    <div class="actions">
      <%= f.submit "更新" %>
    </div>
    
  <% end %>